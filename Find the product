
import java.util.*;

class TestClass {
    public static void main(String args[] ) throws Exception {
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int[] a=new int[n];
		
	 long sum=1;
		for(int i=0;i<a.length;i++)
		{
			a[i]=s.nextInt();
		}
		s.close();
	    for(int ele:a)
			sum=(sum*ele)%(1000000007);
		
		System.out.println(sum);
		
		}
	




}


